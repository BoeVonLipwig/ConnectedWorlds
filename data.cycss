node.project {
	shape : ellipse;
	text-margin-y : 1px;
	font-weight: 300;
	font-style: normal;
	text-transform: uppercase;
	text-outline-width: 2;

	text-valign: center;

	background-color: #287777;
	background-opacity : 0;
	border-width: 2px;
	border-color: #287777;

	width: 30;
	height: 30;
}




node[type = "center"] {
opacity :0;
}

edge.faded {
	opacity: 0.1;
}

node.faded {
	opacity: 0.5;
	text-opacity: 0;
}

node[type = "center"].faded {
opacity :0;
}

node.hidden{
	opacity : 0;
}

node.highlighted {
	min-zoomed-font-size: 0;
    	font-size: 10;
    	text-outline-width: 2;
    	text-outline-opacity: 1;
    	z-index: 9999;
    	line-color: #fff;
    	opacity : 1;
}

node.hover-hood {
    	text-outline-opacity: 1;
    	opacity : 1;
}

node:selected {
	width: 30;
	height: 30;
	border-color: #fff;
	border-opacity: 1;
	border-width: 4px;
	opacity : 1;
}

 node.hover {
	border-color: #fff;
	border-width: 4px;
}

