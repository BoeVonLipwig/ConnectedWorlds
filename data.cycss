node[type = "key"]{
	events : no;
}

edge.hidden{
	opacity: 0;
}

edge.highlighted {
	opacity: 1;
	width: 1;
	/*z-index: 9999;*/
}

edge.hover-hood {
	opacity: 1;
	width: 1;
	/*z-index: 9999;*/
}



edge.hover-hood[type = "collab"] {
	opacity: 1;
	line-color: #78909C;
	z-index: 5;
}

edge.highlighted[type = "collab"] {
	opacity: 1;
	line-color: #ECEFF1;
}

#titleKey {
	font-weight: 400;
	font-style: normal;
